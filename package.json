{
  "name": "shadcn-ui-monorepo",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "next": "^15.5.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.7.3"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.1",
      "react-dom": "19.1.1",
      "next": "15.5.0",
      "next-themes": "0.4.6"
     
    }
  }
}

